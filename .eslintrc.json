{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true, 
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 1,
        "no-plusplus": 0,
        "no-restricted-globals": 1,
        "no-undef": 0,
        "import/no-extraneous-dependencies": 1
    }
}
